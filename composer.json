{
    "name" : "ammonkc/ptpkg",
    "description" : "A wordpress plugin for ptpkg api client",
    "version": "0.1.0-dev",
    "type" : "wordpress-plugin",
    "keywords" : ["wordpress"],
    "license" : "MIT",
    "homepage" : "http://github.com/ammonkc/wp-ptpkg",
    "authors" : [
        {
            "name" : "Ammon Casey",
            "email" : "ammon@caseyohana.com",
            "role" : "Lead Developer"
        }
    ],
    "repositories": {
        "ptpkg-api": {
            "type": "path",
            "url": "/Users/ammonkc/Develop/src/ptpkg-api",
            "options": {
                "symlink": true
            }
        }
    },
    "require" : {
        "php" : ">=5.6.0",
        "ammonkc/ptpkg-api": "@dev",
        "guzzlehttp/guzzle": "^6.3",
        "gamajo/template-loader": "^1.3",
        "doctrine/inflector": "^1.3"
    },
    "autoload" : {
        "psr-4" : {
            "Ptpkg\\" : "src"
        }
    }
}
