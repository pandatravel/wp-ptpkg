{
    "name" : "ammonkc/ptpkg",
    "description" : "A wordpress plugin for Ptpkg API v1",
    "version": "1.0.2",
    "type" : "wordpress-plugin",
    "keywords" : ["ptpkg-api"],
    "license" : "UNLICENSED",
    "homepage" : "http://github.com/ammonkc/wp-ptpkg",
    "authors" : [
        {
            "name" : "Ammon Casey",
            "email" : "acasey@panda-group.com",
            "role" : "Lead Developer"
        }
    ],
    "repositories": {
        "ptpkg-api": {
            "type": "vcs",
            "url": "git@github.com:pandatravel/ptpk-api.git"
        }
    },
    "require" : {
        "php" : ">=5.6.0",
        "ammonkc/ptpkg-api": "^1.0",
        "gamajo/template-loader": "^1.3",
        "nesbot/carbon": "^1.22",
        "firebase/php-jwt": "^5.0",
        "doctrine/inflector": "^1.1"
    },
    "autoload" : {
        "psr-4" : {
            "Ptpkg\\" : "src"
        }
    }
}
